{
  "extends": "eslint-config-airbnb",
  "rules": {
    "semi": [2, "never"],
    "eqeqeq": [2, "smart"],
    "no-unexpected-multiline": 2,
    "comma-dangle": [2, "never"]
  },
  "parser": "babel-eslint",
  "env": {
    "mocha": true
  },
  "globals": {
    "__DEV__": true,
    "__TEST__": true
  }
}
